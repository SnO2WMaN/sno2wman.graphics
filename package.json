	"scripts": {
		"dev": "./node_modules/.bin/nuxt --hostname 0.0.0.0 --port 3000",
		"build": "./node_modules/.bin/nuxt build",
		"start": "./node_modules/.bin/nuxt start",
		"generate": "./node_modules/.bin/nuxt generate",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"lint:eslint": "./node_modules/.bin/eslint . --ext .js,.vue --ignore-path .gitignore",
		"lint:stylelint": "./node_modules/.bin/stylelint **/*.vue **/*.s?css --ignore-pattern .gitignore"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"browserslist": [
		">0.25%",
		"not ie 11",
		"not op_mini all",
		"not dead"
	],
	"dependencies": {
		"awesome-sass-easing": "^1.1.2",
		"mathsass": "^0.10.1",
		"minireset.css": "^0.0.3",
		"nuxt": "^2.2.0",
		"nuxt-sass-resources-loader": "^2.0.5",
		"sass-metallic-ratio": "^0.0.3"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.1",
		"babel-preset-env": "^1.7.0",
		"eslint": "^5.7.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-plugin-vue": "^5.0.0-beta.3",
